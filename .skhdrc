# Custom keyboard shortcuts to spawn apps

#ctrl - p : osascript -e 'tell application "Cursor" to activate'
ctrl - p : open -a 'Visual Studio Code'

#ctrl - s : osascript -e 'tell application "Safari" to activate'
ctrl - s : open -a 'Safari'
#ctrl - m : osascript -e 'tell application "Google Chrome" to activate'
ctrl - m : open -a 'Google Chrome'

#ctrl - a : osascript -e 'tell application "Telegram" to activate'
ctrl - a : open -a 'Telegram'

#ctrl - t : osascript -e 'tell application "iTerm" to activate'
ctrl - t : open -a 'iTerm'
